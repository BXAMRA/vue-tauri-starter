{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "vue-tauri-starter",
  "version": "0.0.1",
  "identifier": "com.company.app-name",
  "build": {
    "frontendDist": "../app",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "vue-tauri-starter",
        "resizable": false,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
